<script>

frm = $('#form_forgot');
frm.submit(function (e) {

     e.preventDefault();

     jQuery.ajax({
     type: "POST",
     url: "https://www.occ-france.com/membre/login/mdp",
     data:frm.serialize(),
     dataType: "json",
     success: function(data) {

        if(data.res=="oui"){
          $('.txt-success').show();
          $('.txt-email,.btn-block,.txt-intro').hide();
        }
        else {
          $('.txt-email').show();
          $('.txt-error').show();
        }
       
      }
      });
   
 
});
   </script>
